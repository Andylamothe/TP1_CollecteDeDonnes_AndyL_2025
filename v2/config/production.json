{
    "server": {
        "port": 8080
    },
    "db": {
        "uri": "MONGO_URI"
    },
    "logging": {
        "level": "warn",
        "file": {
            "enabled": true,
            "path": "/var/log/tv_tracker/app.log"
        }
    },
    "swagger": {
        "enabled": true
    },
    "security": {
        "jwt": {
            "expiresIn": "1d"
        }
    },
    "cors": {
        "origin": "CORS_ORIGIN",
        "credentials": true
    },
    "rateLimit": {
        "windowMs": 900000,
        "max": 50,
        "authWindowMs": 900000,
        "authMax": 5
    }
}
